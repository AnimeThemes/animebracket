<dl data-id="{{id}}" class="entrant-info">
    <dt>Headshot</dt>
    <dd class="image">
        <img src="http://cdn.awwni.me/bracket/{{image}}" alt="{{name}}" />
    </dd>
    <dd class="name">{{name}}</dd>
    <dd class="source">{{source}}</dd>
    <dd class="votes">{{votes}} votes</dd>
</dl>