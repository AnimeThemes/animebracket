[
    {
        "rule": "^admin/$",
        "replace": "page=admin"
    },
    {
        "rule": "^verify$",
        "replace": "page=user&action=verify_oauth"
    },
    {
        "rule": "^login$",
        "replace": "login/",
        "redirect": true
    },
    {
        "rule": "^login/$",
        "replace": "page=user"
    },
    {
        "redirect": true,
        "rule": "^([^/.]+)/nominate$",
        "replace": "$1/nominate/"
    },
    {
        "rule": "^([^/.]+)/nominate/$",
        "replace": "page=bracket&action=nominate&bracket=$1"
    },
	{
		"rule": "^tier/([0-9]+)$",
		"replace": "tier/$1/",
		"redirect": true
	},
	{
		"rule": "^tier/([0-9]+)/$",
		"replace": "page=bracket&action=tier"
	},
    {
        "rule": "^([^/.]+)/vote$",
        "replace": "$1/vote/",
        "redirect": true
    },
    {
        "rule": "^([^/.]+)/vote/$",
        "replace": "page=bracket&action=vote&bracket=$1"
    },
	{
		"rule": "^([^/.]+)/view$",
		"replace": "$1/view/",
		"redirect": true
	},
	{
		"rule": "^([^/.]+)/view/$",
		"replace": "page=bracket&action=view&bracket=$1"
	}
]
