[
    {
        "rule": "^admin/$",
        "replace": "page=admin"
    },
    {
        "rule": "^admin/create/$",
        "replace": "page=admin&action=create"
    },
    {
        "rule": "^admin/([^/]+)/([^/]+)/$",
        "replace": "page=admin&perma=$1&action=$2"
    },
    {
        "rule": "^admin/([^/]+)/([^/]+)/([^/]+)/$",
        "replace": "page=admin&perma=$1&action=$2&state=$3"
    },
    {
        "rule": "^verify$",
        "replace": "page=user&action=verify_oauth"
    },
    {
        "rule": "^submit/$",
        "replace": "page=votesubmit"
    },
    {
        "rule": "^typeahead/$",
        "replace": "page=typeahead"
    },
    {
        "rule": "^login$",
        "replace": "login/",
        "redirect": true
    },
    {
        "rule": "^login/$",
        "replace": "page=user"
    },
    {
        "rule": "^brackets$",
        "redirect": true,
        "replace": "/brackets/"
    },
    {
        "rule": "^brackets/$",
        "replace": "page=bracket&action=all"
    },
    {
        "rule": "^stats/$",
        "replace": "page=stats"
    },
    {
        "redirect": true,
        "rule": "^([^/.]+)/nominate$",
        "replace": "$1/nominate/"
    },
    {
        "rule": "^([^/.]+)/nominate/$",
        "replace": "page=bracket&action=nominate&bracket=$1"
    },
    {
        "rule": "^([^/.]+)/vote$",
        "replace": "$1/vote/",
        "redirect": true
    },
    {
        "rule": "^([^/.]+)/vote/$",
        "replace": "page=bracket&action=vote&bracket=$1"
    },
    {
        "rule": "^([^/.]+)/view$",
        "replace": "$1/view/",
        "redirect": true
    },
    {
        "rule": "^([^/.]+)/view/$",
        "replace": "page=bracket&action=view&bracket=$1"
    },
	{
		"rule": "^([^/.]+)/characters$",
		"replace": "$1/characters/",
		"redirect": true
	},
	{
		"rule": "^([^/.]+)/characters/$",
		"replace": "page=bracket&action=characters&bracket=$1"
	},
    {
        "rule": "^api/([^/.]+)/$",
        "replace": "page=jsonapi&action=$1"
    }
]
